#define DX_TRACK_TEXTURE_HANDLER
(
;{dx_log_writer FALSE {sprint "Setting Custom Track Textures"}}
{unless {modifier_mgr is_modifier_active mod_nohud} ;don't run anything if performance mode is enabled
DX_ANIMATED_TASK_KILLER

;populate memory with animated gems textures for replacement
{if $dx_animated_gem_needs_reset ;if we have an animated gem update queued
   {if $dx_animated_gems_enabled ;if we have animated gems enabled
      {dx_log_writer FALSE {sprint "Animated Gems are on. Pain:" $dx_animated_gem}}
      {set $dx_animated_gem_texture_frame_count 0} ;we need to ensure we start at 0 before running the following task
      {thread_task kTaskSeconds ;create a new task
         (script ;task contents
            {if {|| {$user get_track_type} kTrackDrum kTrackGuitar kTrackBass kTrackKeys kTrackRealKeys} ;check the currently loaded instrument
               {new Tex {sprint "gemframe" $dx_animated_gem_texture_frame_count ".tex"}} ;create a new texture with a numerical index of the currently tracked variable for it's specific instrument gem type
               {{sprint "gemframe" $dx_animated_gem_texture_frame_count ".tex"} set_bitmap {sprint "ui/track/animated_gems/prism_gems_" $dx_animated_gem "/prism_gems_" $dx_animated_gem "_" $dx_animated_gem_texture_frame_count ".png"}} ;populate the currently created texture index with the selected animated gem index for it's specific instrument gem type
            }
            {if {== {$user get_track_type} kTrackDrum} ;check the currently loaded instrument
               {new Tex {sprint "gemdrumframe" $dx_animated_gem_texture_frame_count ".tex"}} ;create a new texture with a numerical index of the currently tracked variable for it's specific instrument gem type
               {{sprint "gemdrumframe" $dx_animated_gem_texture_frame_count ".tex"} set_bitmap {sprint "ui/track/animated_gems/gem_cymbal_diffuse_" $dx_animated_gem "/gem_cymbal_diffuse_" $dx_animated_gem "_" $dx_animated_gem_texture_frame_count ".png"}} ;populate the currently created texture index with the selected animated gem index for it's specific instrument gem type
            }
            {if {== {$user get_track_type} kTrackRealKeys} ;check the currently loaded instrument
               {new Tex {sprint "gemkeysframe" $dx_animated_gem_texture_frame_count ".tex"}} ;create a new texture with a numerical index of the currently tracked variable for it's specific instrument gem type
               {{sprint "gemkeysframe" $dx_animated_gem_texture_frame_count ".tex"} set_bitmap {sprint "ui/track/animated_gems/prism_gem_keyboard_style_" $dx_animated_gem "/prism_gem_keyboard_style_" $dx_animated_gem "_" $dx_animated_gem_texture_frame_count ".png"}} ;populate the currently created texture index with the selected animated gem index for it's specific instrument gem type
            }
            {set $dx_animated_gem_texture_frame_count {+ $dx_animated_gem_texture_frame_count 1}} ;increment the variable to setup the next texture creation index
            {unless {>= $dx_animated_gem_texture_frame_count 60} ;only run the task until the variable reaches 60
               {$task loop} ;loop the task if variable is below 60
            }
         )
      }
   }
}

;apply animated gems in game task
{if $dx_animated_gems_enabled ;if we have animated gem enabled
   {set $dx_gemframecountdif 0} ;we need to ensure we start at 0 before running the following task
   {thread_task kTaskSeconds ;create a new task
      (name animated_gems) ;name the current task for deletion tracking
      (delay 0.1) ;delay the start of the task
      (script ;task contents
         {if {== {$user get_track_type} kTrackDrum} ;check the currently loaded instrument
            {gem_cymbal_diffuse.tex iterate_refs $ref {$ref set diffuse_tex {sprint "gemdrumframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
            {gem_cymbal_diffuse.tex iterate_refs $ref {$ref set emissive_map {sprint "gemdrumframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
            {gem_cymbal_diffuse.tex iterate_refs $ref {$ref set normal_map {sprint "gemdrumframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
            {gem_cymbal_ems.tex iterate_refs $ref {$ref set diffuse_tex {sprint "gemdrumframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
         }
         {if {|| {$user get_track_type} kTrackDrum kTrackGuitar kTrackBass kTrackKeys kTrackRealKeys} ;check the currently loaded instrument
            {prism_gems.tex iterate_refs $ref {$ref set diffuse_tex {sprint "gemframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
            {prism_gems.tex iterate_refs $ref {$ref set emissive_map {sprint "gemframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
            {prism_gems.tex iterate_refs $ref {$ref set normal_map {sprint "gemframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
            {prism_gem_emissive.tex iterate_refs $ref {$ref set diffuse_tex {sprint "gemframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
         }
         {if {== {$user get_track_type} kTrackRealKeys} ;check the currently loaded instrument
            {prism_gem_keyboard_style.tex iterate_refs $ref {$ref set diffuse_tex {sprint "gemkeysframe" $dx_gemframecountdif '.tex'}}} ;apply the currently selected animated texture index with the selected animated highway index image we pre generated
         }
         {set $dx_gemframecountdif {+ $dx_gemframecountdif 1}} ;increment the variable to setup the next texture application index
         {$task sleep 0.016} ;delay the next step of the task for roughly 1 frame at 60fps
         {if {>= $dx_gemframecountdif 60} {set $dx_gemframecountdif 0}} ;only run the task until the variable reaches 60
         {$task loop} ;loop the task
      )
   }
}

