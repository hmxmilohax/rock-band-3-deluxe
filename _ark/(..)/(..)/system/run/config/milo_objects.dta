(Song
   (description
      "The Song Milo editor")
   (editor
      (song
         symbol
         (list
            {fake_songmgr get_song_list})
         #ifdef HX_PC
         (post_sync
            {script_task
               kTaskBeats
               (delay 0)
               (script
                  {milo editor_spec_changed $this})})
         #endif)
      (loop
         (indent
            (set_start
               script
               (script
                  {$this
                     set_loop_start
                     {$this frame}}
                  #ifdef HX_PC
                  {milo update_open_editor $this}
                  #endif)
               (help
                  "Sets the current frame of the song to be the start of the loop."))
            (loop_start
               (vector
                  (m int)
                  (b int)
                  (t int))
               (read_only TRUE))
            (set_end
               script
               (script
                  {$this
                     set_loop_end
                     {$this frame}}
                  #ifdef HX_PC
                  {milo update_open_editor $this}
                  #endif)
               (help
                  "Sets the current frame of the song to be the end of the loop."))
            (loop_end
               (vector
                  (m int)
                  (b int)
                  (t int))
               (read_only TRUE))
            (reset
               script
               (script
                  {$this set_loop_start 0}
                  {$this
                     set_loop_end
                     {$this end_frame}}
                  #ifdef HX_PC
                  {milo update_open_editor $this}
                  #endif)
               (help
                  "Resets the looping."))))
      (sections
         (struct))
      (speed float)
      (song_end_frame
         float
         (read_only TRUE))
      (debug_parsers
         (array
            object
            (class Object)
            (list
               {$this get_midi_parsers})))))
(EditTimeline
   (description
      "The PropAnim Milo editor")
   (types
      (edit_timeline
         #include ../milo/propanim/propanim.dta)))