      #ifndef _SHIP
      {if
         {$this show_asset_tokens}
         {cheat_display
            show
            {$component selected_sym}}}
      #endif
      {switch
         $component
         (new.lst
            {if
               {>=
                  {new.lst num_data}
                  0}
               {set
                  $name
                  {new.lst selected_sym}}
               {closet_mgr reset_new_character_preview $name}
               {$this preview_asset $name}
               {customize_clothing_panel
                  goto_shot
                  {$this get_asset_shot $name}}})
         (current_outfit.lst
            {if
               {>=
                  {current_outfit.lst num_data}
