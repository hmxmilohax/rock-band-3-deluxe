               {set
                  $name
                  {current_outfit.lst selected_sym}}
               {$this setup_current_outfit $name}
               {$this refresh_patch_help $name}
               {$this refresh_customizable}
               {customize_clothing_panel
                  goto_shot
                  {$this get_asset_shot $name}}})
         (assets.lst
            {if
               {>=
                  {assets.lst num_data}
                  0}
               {$this
                  preview_asset
                  {assets.lst selected_sym}}})
         (premium_assets.lst
            {if
               {>=
                  {premium_assets.lst num_data}
                  0}
               {set
                  $name
                  {premium_assets.lst selected_sym}}
               {$this preview_asset $name}
               {$this refresh_premium_help $name}})
         (makeup.lst
            {if
               {>=
                  {makeup.lst num_data}
                  0}
               {$this
                  preview_makeup
                  {makeup.lst selected_sym}}})
         (tattoos.lst
            {$this
               goto_tattoo_shot
               {tattoos.lst selected_sym}})
         (boutiques.lst
            {set
               $boutique
               {boutiques.lst selected_sym}}
            {set
               $state
               {$this get_state}}
            {if_else
               {&&
                  {== $boutique boutique_tshirts}
                  {!= $state kCustomizeState_BrowseTorso}}
               {boutiques.lst scroll 1}
               {do
                  {$this
                     set_current_boutique
                     {boutiques.lst selected_sym}}}})})
   (BUTTON_DOWN_MSG
      {set
         $state
         {$this get_state}}
      {if
         {contains
            [modify_states]
            $state}
         {switch
            $action
            (kAction_Up
               {if
                  {!= $state kCustomizeState_PatchRotate}
                  {play_instr_sfx $user button_toggle}
                  {arrow_up.mnm set_frame 1}})
            (kAction_Down
               {if
                  {!= $state kCustomizeState_PatchRotate}
                  {play_instr_sfx $user button_toggle}
                  {arrow_down.mnm set_frame 1}})
            (kAction_Left
               {if
                  {!= $state kCharCreatorState_FaceMakerRotateEyes}
                  {play_instr_sfx $user button_toggle}
                  {arrow_left.mnm set_frame 1}})
            (kAction_Right
               {if
                  {!= $state kCharCreatorState_FaceMakerRotateEyes}
                  {play_instr_sfx $user button_toggle}
                  {arrow_right.mnm set_frame 1}})
            kDataUnhandled}}
      {switch
