                  {$this set_state kCustomizeState_PatchMenu}}})
         kDataUnhandled})
   (BUTTON_UP_MSG
      {cond
         ({contains
               [modify_states]
               {$this get_state}}
            {switch
               $action
               (kAction_Up
                  {arrow_up.mnm set_frame 0})
               (kAction_Down
                  {arrow_down.mnm set_frame 0})
               (kAction_Left
                  {arrow_left.mnm set_frame 0})
               (kAction_Right
                  {arrow_right.mnm set_frame 0})
               kDataUnhandled})
         kDataUnhandled})}
