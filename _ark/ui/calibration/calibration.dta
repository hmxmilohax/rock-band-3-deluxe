{func
   increment_to_1mult
   ($value)
   {'*'
      {'+'
         {floor
            {/ $value 1.0}}
         1}
      1}}
{func
   decrement_to_1mult
   ($value)
   {'*'
      {-
         {ceil
            {/ $value 1.0}}
         1}
      1}}
