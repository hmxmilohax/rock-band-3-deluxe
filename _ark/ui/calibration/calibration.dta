   (focus speed_quick_panel)
   (back main_hub_screen)}
{new
   UIPanel
   fps_quick_panel
   (file
      "cal_quick.milo")
   (focus audio_lag.sld)
   (enter
      {audio_lag.sld set_num_steps 60} ;number of steps for venue fps slider
      {audio_lag.sld set_current {int {- $emulatefps 1}}} ;set FPS slider based on current value
      {calibration_options.lbl set_token_fmt "Set Venue FPS"}
      {audio_lag.lbl set_token_fmt {sprintf {localize fps_msg} {int $emulatefps}}} ;set venue fps msg
      {do {video_lag.lbl set_showing FALSE} {$this disable video_lag.sld} {video_lag.sld set_showing FALSE}}
     {$this update_all_from_profile}
     )
   (display_fps_msg
      ($fps)
     {audio_lag.lbl set_token_fmt {sprintf {localize fps_msg} {int $fps}}}) ;don't add 1 here because this is being fed the actual fps
   (exit)
   (SCROLL_SELECT_MSG
      {unless
         $selected
         {$this on_slider_change $component}})
   (SCROLL_MSG
      {$this on_slider_change $component})
   (set_speeds
         {do
            {set $emulatefps
               {+ 1 {audio_lag.sld current}}}}
         {$this display_fps_msg $emulatefps})
   (on_slider_change
      ($component)
      {switch
         $component
         (audio_lag.sld
            {do {set $component current}}
            {$this set_speeds})})
   (update_all_from_profile
      {$this
         iterate
         UISlider
         $s
         {$this update_from_profile $s}})
   (update_from_profile
      ($component)
      {switch
         $component
         (audio_lag.sld
            {$component
               set_current
               {int {- $emulatefps 1}}})})}
