{new
   UIPanel
   speed_quick_panel
   (file
      "cal_quick.milo")
   (focus audio_lag.sld)
   (enter
      {audio_lag.sld set_num_steps 41} ;number of steps for note speed slider
      {video_lag.sld set_num_steps 31} ;number of steps for song speed slider
      {audio_lag.sld set_current {int {+ 0.5 {/ {+ {- $trackspd 1} 0.5} 0.05}}}} ;set note speed slider based on current value
      {video_lag.sld set_current {int {+ 0.5 {/ {- $speedmod 1} 0.05}}}} ;set song speed slider based on current value
      {calibration_options.lbl set_token_fmt "Set Speeds"}
      {audio_lag.lbl set_token_fmt {sprintf {localize trackspeed_msg} {int {+ 0.5 {* $trackspeed 100}}}}} ;set note speed msg
      {video_lag.lbl set_token_fmt {sprintf {localize speed_msg} {int {+ 0.5 {* $speedmod 100}}}}} ;set song speed msg
     {$this update_all_from_profile}
     )
   (display_trackspeed_msg
      ($tspd)
      {audio_lag.lbl set_token_fmt {sprintf {localize trackspeed_msg} {int {+ 0.5 {* $tspd 100}}}}})
   (display_speed_msg
      ($mspd)
     {video_lag.lbl set_token_fmt {sprintf {localize speed_msg} {int {+ 0.5 {* $mspd 100}}}}})
   (exit)
   (SCROLL_SELECT_MSG
      {unless
         $selected
         {$this on_slider_change $component}})
   (SCROLL_MSG
      {$this on_slider_change $component})
   (set_speeds
         {do
            {set $trackspeed {- {+ 1 {* {audio_lag.sld current} 0.05}} 0.5}}
            {set $speedmod
               {+ 1 {* {video_lag.sld current} 0.05}}}
         {set $leaderspeed $speedmod}}
         {set $leaderspeed $speedmod}
         {$this display_trackspeed_msg $trackspeed}
         {$this display_speed_msg $speedmod})
   (on_slider_change
      ($component)
      {switch
         $component
         (audio_lag.sld
            {do {set $component current}}
            {$this set_speeds})
         (video_lag.sld
            {do {set $component current}}
            {$this set_speeds})})
   (update_all_from_profile
      {$this
         iterate
         UISlider
         $s
         {$this update_from_profile $s}})
   (update_from_profile
      ($component)
      {switch
         $component
         (audio_lag.sld
            {$component
               set_current
               {int {+ 0.5 {/ {+ {- $trackspeed 1} 0.5} 0.05}}}})
         (video_lag.sld
            {$component
               set_current
               {int {+ 0.5 {/ {- $speedmod 1} 0.05}}}})})}
