#define CAL_MANUAL_PANEL_CONTENTS
((file cal_manual.milo)
   (enter
      {$this add_sink cal_background_panel}
      {if_else
         [is_video]
         {do
            {audio_labels.grp set_showing FALSE}
            {video_labels.grp set_showing TRUE}
            {set
               [get_method]
               get_excess_video_lag}
            {set
               [set_method]
               set_excess_video_lag}}
         {do
            {audio_labels.grp set_showing TRUE}
            {video_labels.grp set_showing FALSE}
            {set
               [get_method]
               get_excess_audio_lag}
            {set
               [set_method]
               set_excess_audio_lag}}}
      {set
         [old_latency]
         {profile_mgr
            [get_method]}}
      {$this
         set_latency
         {profile_mgr
            [get_method]}}
      {set
         [parent]
         {ui current_screen}}
      {$this
         set_focus
         ""})
   (BUTTON_DOWN_MSG
      {switch
         $action
         (kAction_Cancel
            {$this
               set_latency
               [old_latency]}
            kDataUnhandled)
         ((kAction_Up kAction_Right)
            {play_common_seq slider.cue}
            {$this
               set_latency
               {increment_to_1mult
                  {profile_mgr
                     [get_method]}}})
         ((kAction_Down kAction_Left)
            {play_common_seq slider.cue}
            {$this
               set_latency
               {decrement_to_1mult
                  {profile_mgr
                     [get_method]}}})
         kDataUnhandled})
   (set_latency
      ($latency)
      {if
         {&&
            {> $latency -301}
            {< $latency 301}}
         {profile_mgr
            [set_method]
            $latency}
         {current_offset.lbl
            set_token_fmt
            (cal_manual_offset
               {int $latency})}}))
