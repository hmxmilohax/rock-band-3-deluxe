{new
   UIPanel
   mydta_start_in_game_panel
   (state begin)
   (enter
      {set
         [state]
         begin})
   (TRANSITION_COMPLETE_MSG
      {set
         [state]
         saveload}
      {saveload_mgr activate}
      {unless
         {'||'
            {! $gotcha}
            {modifier_mgr is_modifier_active mod_auto_play}}
         {saveload_mgr autosave}})
   (poll
      {switch
         [state]
         (saveload
            {if
               {saveload_mgr is_initial_load_done}
               {set
                  [state]
                  contentrefresh}
               {content_mgr start_refresh}})
         (contentrefresh
            {if
               {content_mgr refresh_done}
               {set
                  [state]
                  done}
               {{ui current_screen}
                  ready_for_game}})})}
