      {if
         {&&
            $ok
            $this
            {!
               {==
                  $text
                  ""}}}
         {$this rename_character $text}
         {$this show_chars}
         {unless {|| {! $gotcha} {modifier_mgr is_modifier_active mod_auto_play}} {saveload_mgr autosave}}
