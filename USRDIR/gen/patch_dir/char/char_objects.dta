                  {set
                     [default_loop]
                     kPlayLoop})
               (face_creator
                  {set
                     [beat_align]
                     kPlayRealTime}
                  {unless
                     {==
                        {$this name}
                        base}
                     {set
                        [relative]
                        {{$this dir}
                           find
