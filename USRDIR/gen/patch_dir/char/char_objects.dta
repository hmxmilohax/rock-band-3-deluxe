         {set
            $origPath
            {find_original_path $this}}
         {search_replace
            $origPath
            "/run/"
            "/run/anim_src/"
            $animsrcPath}
         {if_else
            {&&
               {file_exists $animsrcPath}
               {!
                  {has_substr
                     $origPath
                     "anim_src"}}}
            {do
               {set
                  $clipDir
                  {load_objects $animsrcPath}}
               {if_else
                  $clipDir
                  {do
                     {$clipDir perforce_sync $animsrcPath}
                     {{$clipDir
                           find
                           {$this name}}
                        set
                        viseme_group
                        [viseme_group]}
                     {$clipDir perforce_resave}
                     {delete $clipDir}
                     {filter_notify
                        $msg
                        {has_substr
                           $msg
                           "Merging"}
                        {merge_back $this false}}
                     {notify
                        "Success!\nSaved here:\n"
                        $animsrcPath
                        "\n\nAnd here:\n"
                        $origPath}}
                  {notify
                     "Error: couldn't load clips from here: \n"
                     $animsrcPath}}
               '\\'}
            {notify
               "Error: can't find anim_src path for clip, expected it to be here:\n"
               $animsrcPath}})
