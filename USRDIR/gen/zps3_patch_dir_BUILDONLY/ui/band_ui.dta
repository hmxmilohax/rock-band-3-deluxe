            ($reason)
            {forfeit_ranked_game})
         (on_dismiss
            ($reason)
            {ui_event_mgr
               trigger_event
               error_message
               (dummy
                  init
                  {switch
                     $reason
                     (kLostConnection error_lost_connection)
                     (kLostServer error_lost_server)
                     (kEthernetUnplugged error_ethernet_unplugged)})}))
      (no_online_privileges
         (next_screen meta_loading_uisink_lost_connection_screen)
